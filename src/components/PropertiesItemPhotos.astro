---
import PhotosGalery from "@/components/PhotosGalery";

const { proptype, photos } = Astro.props;
---

<div class="relative w-full group/photo h-72">
  <div
    class="absolute top-6 left-0 h-11 text-base bg-primary-800 text-white pl-6 pr-10 rounded-e-2xl flex items-center z-20"
  >
    {proptype == "Residential" ? "For Sale" : "For Rent"}
  </div>

  <PhotosGalery client:load photos={photos} />
</div>
